import java.util.*;

public class Goblin extends Enemy{

    private int dam;
    
    public String toString(){
	return "Goblin";
    }

    public Goblin(){
	setXP(25);
	dam = Random.nextInt(5) + 5;
    }
    
    public void attack(Character a){
	System.out.println(this + " attacked " + a);
	System.out.println("did " + dam + " damage");
    }

    
}
