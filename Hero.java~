public abstract class Hero extends Character{
    
    private int XP;
    private int money;
    private int level;

    public int getMoney(){
	return money;
    }

    public void changeMoney(int a){
	money += a;
    }

    public int getXP(){
	return XP;
    }

    public void changeXP(int a){
	money += XP;
    }

    

    
